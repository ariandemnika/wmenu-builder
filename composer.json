{
    "name": "arian/laravel-menu",
    "description": "Drag and drop menu generator like WordPress for Laravel supporting versions 5 to 10.30",
    "keywords": ["Laravel", "Menu", "WordPress", "Builder"],
    "authors": [
        {
            "name": "Rendy Harimayco",
            "email": "rendyharimayco@gmail.com",
            "role": "Developer"
        },
        {
            "name": "Cristian Garcia",
            "email": "informacion@cristiangarcia.co",
            "role": "Original Developer"
        }
    ],
    "require": {
        "php": ">=8.2.4",
        "illuminate/support": "5.* || 6.* || 7.* || 8.* || 9.* || 10.*",
        "arian/laravel-menu": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Harimayco\\Menu\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Harimayco\\Menu\\MenuServiceProvider"
            ],
            "aliases": {
                "Menu": "Harimayco\\Menu\\Facades\\Menu"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
